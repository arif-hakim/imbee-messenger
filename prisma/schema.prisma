generator client {
  provider = "prisma-client"
  output   = "../src/db/generated/client"
}

datasource db {
  provider = "mysql"
}

model FcmJob {
  identifier String   @id @unique
  type       String
  deviceId   String
  text       String
  deliverAt  DateTime @default(now())
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt

  @@map("fcm_job")
}
